{
  "name": "MonologueLibrarian",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/agawley/MonologueLibrarian.git",
  "license": "MIT",
  "scripts": {
    "test": "TZ=Europe/London jest"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "csv-parse": "^4.3.0",
    "easymidi": "^1.0.2"
  },
  "eslintConfig": {
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "it": false,
        "describe": false,
        "expect": false,
        "jest": false,
        "afterAll": false,
        "beforeAll": false,
        "chrome": false
    },
    "rules": {
        "semi": ["error", "always"],
        "no-var": "error",
        "prefer-const": ["error", {
            "destructuring": "any",
            "ignoreReadBeforeAssign": false
        }],
        "sort-imports": "error"
      }
  }
}
